{
    "name": "iexbase/tron-api",
    "description": "A PHP API for interacting with Tron (Trx)",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/underworf/tron-api",
    "authors": [
        {
            "name": "Shamsudin Serderov",
            "email": "steein.shamsudin@gmail.com"
        }
    ],
    "keywords": [
        "iexbase",
        "tron-lib",
        "tron-php",
        "tron-api",
        "tron-rest-api"
    ],
    "require": {
        "php": ">=8.0",
        "comely-io/data-types": "*",
        "guzzlehttp/guzzle": "^7.2",
        "iexbase/web3.php": "^2.0.1",
        "kornrunner/secp256k1": "^0.2",
        "simplito/elliptic-php": "^1.0",
        "ext-json": "*",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "IEXBase\\TronAPI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IEXBase\\TronAPI\\Test\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/underworf/data-types"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
